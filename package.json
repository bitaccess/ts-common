{
  "name": "faast-ts-config-library-template",
  "version": "0.0.1",
  "description": "Template for typescript library",
  "main": "dist/index.umd.js",
  "module": "dist/index.es5.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist/",
    "src/"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "docs": "typedoc --options typedoc.json",
    "prebuild": "rimraf dist",
    "build": "tsc --build ./tsconfig.build.json && rollup -c rollup.config.ts && npm run docs",
    "start": "rollup -c rollup.config.ts -w",
    "pretest": "npm run lint",
    "test": "jest --runInBand --detectOpenHandles",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "precommit": "lint-staged",
    "preversion": "bin/preversion.sh",
    "version": "npm run build && git add -Af dist docs",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@types/lodash.camelcase": "^4.3.6",
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.123",
    "@types/node": "^10.11.0",
    "@types/rollup-plugin-json": "^3.0.2",
    "@types/rollup-plugin-sourcemaps": "^0.4.1",
    "coveralls": "^3.0.2",
    "faast-ts-config": "github:go-faast/faast-ts-config",
    "jest": "^23.6.0",
    "jest-config": "^23.6.0",
    "lint-staged": "^8.0.0",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.0",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.18.0",
    "ts-jest": "^23.10.2",
    "ts-lint": "^4.5.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.14.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.3333"
  }
}
